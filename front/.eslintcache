k/Ironhack-final-project/front/src/components/Pages/Products/Products-list.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Pages/Products/Prod-details.js",["103"],"import React, { Component } from 'react'\nimport { Link } from 'react-router-dom'\nimport { Col, Button, Container, Row, Modal } from 'react-bootstrap'\n\nimport Loader from '../../Shared/Spinner'\nimport EditProduct from './Edit-product'\nimport EmailForm from '../../Shared/Email-form'\n\nimport ProductService from '../../../services/products.service'\nimport UserService from '../../../services/user.service'\n\nclass ProductDetails extends Component {\n    constructor(props) {\n        super(props)\n        this.state = {\n            product: undefined,\n            owner: undefined,\n            showDeleteModal: false,\n            showEditModal: false,\n            showContactModal: false,\n            showEmailModal: false,\n            showWppModal: false\n        }\n        this.productsService = new ProductService()\n        this.userService = new UserService()\n    }\n\n    componentDidMount = () => this.loadProducts()\n    \n    loadProducts = () => {\n        this.productsService\n            .getOneProduct(this.props.match.params.product_id)\n            .then(res => this.setState({ product: res.data }, () => this.getOwner()))\n            .catch(err => console.log('ERROR WITH PRODUCT DETAILS', err))\n    }\n\n    getOwner = () => {\n        this.userService\n            .getOneUser(this.state.product.owner)\n            .then(user => this.setState({ owner: user.data }))\n            .catch(err => console.log('ERROR WITH PRODUCT OWNER', err))\n    }\n\n    handleEditModal = visibility => this.setState({ showEditModal: visibility })\n\n    handleDeleteModal = visibility => this.setState({ showDeleteModal: visibility })\n\n    deleteProduct = () => {\n        this.productsService\n            .deleteProduct(this.state.product._id)\n            .then(() => this.props.history.push('/products'))\n            .catch(err => console.log('ERROR DELETIN PRODUCT', err))\n    }\n\n    handleContactModal = visibility => this.setState({ showContactModal: visibility })\n    \n    handleEmailModal = visib => this.setState({ showEmailModal: visib })\n    \n    handleWppModal = visib => this.setState({showWppModal: visib})\n\n    render() {\n        return (\n            <>\n                <Container className=\"product-details\">\n                    {this.state.product\n                        ?\n                        <Row className=\"justify-content-center\">\n                            <Col md={4}>\n                                <img src={this.state.product.image} alt=\"User avatar\" />\n                            </Col>\n                            <Col md={6}>\n                                <h1>{this.state.product.name}</h1>\n                                <hr />\n                                {this.state.owner\n                                    ?\n                                    this.state.owner._id === this.props.theUser._id\n                                            ?\n                                            <h6>Sold by: <Link to={`/profile`}>{this.state.owner.username}</Link></h6>\n                                            :\n                                            <h6>Sold by: <Link to={`/profile/${this.state.owner._id}`}>{this.state.owner.username}</Link></h6>\n                                    \n                                    :\n                                    <>\n                                        <h6>Sold by: Unknown</h6>\n                                        <Loader />\n                                    </>\n                                }\n                                <p>{this.state.product.description}</p>\n                                <h6 className=\"price\">Price: {this.state.product.price}€</h6>\n                                <h6 className=\"status\">{this.state.product.status}</h6>\n                                {this.state.product.owner === this.props.theUser._id\n                                    ?\n                                    <>\n                                        <Button onClick={() => this.handleEditModal(true)} variant=\"secondary\" size=\"sm\" style={{marginRight: '20px'}}>Edit product</Button>\n                                        <Button onClick={() => this.handleDeleteModal(true)} variant=\"danger\" size=\"sm\">Delete product</Button>\n                                    </>\n                                    :\n                                    <Button onClick={() => this.handleContactModal(true)} variant=\"secondary\" size=\"sm\">Show interest</Button>\n                                }\n                            </Col>\n                        </Row>\n                    :\n                        <Loader />\n                    }\n                </Container>\n                {this.state.product\n                    &&\n                    <>\n                    <Modal show={this.state.showEditModal} onHide={() => this.handleEditModal(false)}>\n                        <Modal.Body>\n                            <EditProduct hideModal={() => this.handleEditModal(false)} productId={this.state.product._id} reloadProducts={() => this.loadProducts()} theUser={this.props.theUser} />\n                        </Modal.Body>\n                    </Modal>\n                    <Modal show={this.state.showDeleteModal} onHide={() => this.handleDeleteModal(false)}>\n                        <Modal.Body><b>Are you sure you want to delete this product?</b></Modal.Body>\n                        <Modal.Footer>\n                            <Button variant=\"secondary\" onClick={() => this.handleDeleteModal(false)}>\n                                No, go back\n                            </Button>\n                            <Button variant=\"danger\" onClick={() => this.deleteProduct()}>\n                                Yes, delete\n                            </Button>\n                        </Modal.Footer>\n                    </Modal>\n                    </>\n                }\n                {this.state.owner\n                    &&\n                    <>\n                    <Modal show={this.state.showContactModal} onHide={() => this.handleContactModal(false)}>\n                        <Modal.Body><b>How would you like to contact this seller?</b></Modal.Body>\n                        <Modal.Footer>\n                            <Button variant=\"secondary\" onClick={() => this.handleEmailModal(true)}>\n                                Via Email\n                            </Button>\n                            <a className=\"btn btn-secondary\" target=\"_blank\" href={`https://wa.me/+34${this.state.owner.phone}?text=Mensaje automático de la Patriapp`}>Via WhatsApp</a>\n                        </Modal.Footer>\n                    </Modal>\n                    <Modal show={this.state.showEmailModal} onHide={() => this.handleEmailModal(false)}>\n                        <Modal.Body>\n                            <EmailForm hideModal={() => this.handleEmailModal(false)} toUser={this.state.owner.email} fromUser={this.props.theUser.email} subject={this.state.product.name}/>\n                        </Modal.Body>\n                    </Modal>\n                    </>\n                }\n            </>\n        )\n    }\n}\n\nexport default ProductDetails","/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Pages/Products/New-product.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Pages/User/User-profile.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Pages/People/Users-list.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Pages/Home.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Layout/Navigation.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Pages/User/Signup.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Pages/People/Others-profile.js",["104"],"import React, { Component } from 'react'\nimport { Container, Row, Col, Modal, Button } from 'react-bootstrap'\n\nimport ProductCard from '../Products/Prod-card'\nimport EmailForm from '../../Shared/Email-form'\nimport Loader from '../../Shared/Spinner'\n\nimport ProductService from '../../../services/products.service'\nimport UserService from '../../../services/user.service'\n\nclass OthersProfile extends Component {\n    constructor(props) {\n        super(props)\n        this.state = {\n            user: undefined,\n            products: undefined,\n            showEmailModal: false,\n            showWppModal: false\n        }\n        this.productsService = new ProductService()\n        this.userService = new UserService()\n\n    }\n\n    componentDidMount = () => this.getUser()\n\n    getUser = () => {\n        this.userService\n            .getOneUser(this.props.match.params.userId)\n            .then(user => this.setState({ user: user.data }, () => this.loadProducts()))\n            .catch(err => console.log('ERROR GETING USER', err))   \n    }\n\n    loadProducts = () => {\n        this.productsService\n            .getAllProducts()\n            .then(myProds => this.setState({ products: myProds.data.filter(elm => elm.owner === this.state.user._id) }))\n            .catch(err => console.log('ERROR GETTING PRODS', err))\n    }\n\n    handleEmailModal = visib => this.setState({ showEmailModal: visib })\n    \n    handleWppModal = visib => this.setState({showWppModal: visib})\n\n\n    render() {\n        return (\n            <>\n                {this.state.user\n                    ?\n                    <>\n                        <Container className=\"profile\">\n                            <Row className=\"profile-info\">\n                                <Col md={3}>\n                                    <img src={this.state.user.image} alt=\"User avatar\" />\n                                </Col>\n                                <Col md={9}>\n                                    <h1>{this.state.user.username}'s profile</h1>\n                                    <hr/>\n                                    <h6>Email: {this.state.user.email}</h6>\n                                    <h6>Phone: {this.state.user.phone}</h6>\n                                    <Button onClick={() => this.handleEmailModal(true)} variant=\"secondary\" size=\"sm\">Contact via Email</Button>\n                                    <a className=\"btn btn-secondary btn-sm\" target=\"_blank\" href={`https://wa.me/+34${this.state.user.phone}?text=Mensaje automático de la Patriapp`}>Contact via WhatsApp</a>\n                                </Col>\n                            </Row>\n                            <br/>\n                            <h2>Products</h2>\n                            <hr/>\n                            <Row>\n                                {this.state.products\n                                    ?\n                                    this.state.products.map(elm => <ProductCard key={elm._id} showModal={visib => this.handleModal(visib)} productToEdit={id => this.defineEditProd(id)} product={elm} theUser={this.props.theUser} />)\n                                    :\n                                    <Loader />\n                                }\n                            </Row>\n                        </Container>\n                        <Modal  show={this.state.showEmailModal} onHide={() => this.handleEmailModal(false)}>\n                            <Modal.Body>\n                                <EmailForm hideModal={() => this.handleEmailModal(false)} toUser={this.state.user.email} fromUser={this.props.theUser.email} subject=\"\"/>\n                            </Modal.Body>\n                        </Modal>\n                    </>\n                    :\n                    <Loader />\n                }\n            </>\n        )\n    }\n}\n\nexport default OthersProfile","/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Pages/User/Login.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Pages/User/Edit-profile.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/services/user.service.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/services/products.service.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Pages/Products/Edit-product.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Pages/People/User-card.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Shared/Email-form.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Layout/Searchbar.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Pages/Products/Prod-card.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Shared/Spinner.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Layout/Footer.js",[],"/Users/patriciamdm/Documents/Ironhack/Ironhack-final-project/front/src/components/Layout/Carousel/Carousel.js",[],{"ruleId":"105","replacedBy":"106"},{"ruleId":"107","replacedBy":"108"},{"ruleId":"109","severity":1,"message":"110","line":136,"column":62,"nodeType":"111","endLine":136,"endColumn":77},{"ruleId":"109","severity":1,"message":"110","line":63,"column":77,"nodeType":"111","endLine":63,"endColumn":92},"no-native-reassign",["112"],"no-negated-in-lhs",["113"],"react/jsx-no-target-blank","Using target=\"_blank\" without rel=\"noreferrer\" is a security ri